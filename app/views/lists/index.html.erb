
<%= render 'components/banner' %>
<div id="list-path">

<div class="card-container">
  <% @lists.each do |list| %>
    <div class="card">
    <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>

      <h4><%= link_to list.name, list_path(list) %></h4>
    </div>
  <% end %>
</div>
<div class="body">

</div>
</div>
